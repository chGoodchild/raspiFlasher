config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc/fe300000.mmcnr/mmc_host/mmc1/mmc1:0001/mmc1:0001:1'
	option channel '36'
	option band '5g'
	option htmode 'VHT80'
	option country 'PT'
	option disabled '0'

config wifi-iface 'sta'
	option device 'radio0'
	option network 'wwan'
	option mode 'sta'
	option ssid 'AG'
	option encryption 'psk2'
	option key '21energyminer'

config wifi-iface 'ap'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'psk2'
	option key 'keepout!'

