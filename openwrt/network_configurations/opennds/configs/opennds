    option ndsctl_sock 'ndsctl.sock'
    option authentication_mark '0x00030000'
    option binauth '/usr/lib/opennds/binauth_log.sh'
    option faspath '/'
    option themespec_path ''
    option fasremoteip ''
    option fasremotefqdn 'disabled'
    option fas_ssl 'wget'
    option debuglevel '8'
    option sessiontimeout '1800'
    option preauthidletimeout '30'
    option authidletimeout '120'
    option maxclients '250'
    option enable_serial_number_suffix '1'
    option dhcp_default_url_enable '1'
